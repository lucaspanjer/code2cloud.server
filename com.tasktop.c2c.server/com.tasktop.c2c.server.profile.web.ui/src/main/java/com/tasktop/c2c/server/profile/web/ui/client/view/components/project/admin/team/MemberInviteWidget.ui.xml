<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="pm" type="com.tasktop.c2c.server.profile.web.ui.client.resources.ProfileMessages"/>
	<g:HTMLPanel>
			<g:TextBox ui:field="inviteEmail" addStyleNames="text" />
			<g:Button ui:field="inviteButton" addStyleNames="invite button"><ui:text from="{pm.invite}"/></g:Button>
	</g:HTMLPanel>
</ui:UiBinder>
