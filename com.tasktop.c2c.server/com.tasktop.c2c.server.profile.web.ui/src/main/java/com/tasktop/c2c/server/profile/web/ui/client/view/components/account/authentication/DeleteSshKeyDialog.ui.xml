<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field="pm" type="com.tasktop.c2c.server.profile.web.ui.client.resources.ProfileMessages"/>
	<ui:style>
		.popup {
			background: #ebf3f8;
			padding: 20px 30px;
			width: 400px;
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.popup}">
		<p><ui:text from="{pm.deleteSshKeyConfirm}"/></p>
		<p><g:Label ui:field="keyLabel"/></p>
		<div class="center">
			<g:Button ui:field="okButton" addStyleNames="button special"><ui:text from="{pm.ok}"/></g:Button>
			<g:Button ui:field="cancelButton" addStyleNames="button cancel"><ui:text from="{pm.cancel}"/></g:Button>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>