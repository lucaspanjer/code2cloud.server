<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:d='urn:import:com.tasktop.c2c.server.profile.web.ui.client.view.deployment'>
	<g:HTMLPanel>
	
	
	<div class="content right"> 
	  <div class="deployment"> 
	    <div class="deployment-heading"> 
   	    <h2 class="left">Edit: <g:Label ui:field="title"/></h2> <!-- TODO make editable? -->
		<div class="save"> 
			<g:Anchor ui:field='saveButton1' styleName="right button special">Save</g:Anchor>
			<g:Anchor ui:field='cancelButton1' styleName="right button">Cancel</g:Anchor>
	  </div>					    
	    <div class="clear"></div> 
	  </div> 
			
		<div class="credentials edit">  
			<h3>Credentials</h3>
			<d:CredentialsEditView ui:field="credentialsEditView"/>
		</div>
		<div class="settings edit">  
			<h3>Settings</h3>
			<div class="clear"></div> <!--  WHY? -->
			<d:SettingsEditView ui:field="settingsEditView"/>
		</div>
		<div class="services edit">  
			<h3>Services</h3>
			<d:ServicesEditView ui:field="servicesEditView"/>
		</div>
		<div class="artifact edit">    
			<h3>Artifact</h3>
			<d:ArtifactEditView ui:field="artifactEditView"/>
		</div>
			<div class="save"> 
								<g:Anchor ui:field='saveButton2' styleName="right button special">Save</g:Anchor>
								<g:Anchor ui:field='cancelButton2' styleName="right button">Cancel</g:Anchor>
      				  </div>	
		
		
  						<div class="clear"></div> 
					  </div> 
				  </div> 
				  <div class="clear"></div> 

	</g:HTMLPanel>
</ui:UiBinder>	
