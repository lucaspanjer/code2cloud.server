<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="pm" type="com.tasktop.c2c.server.profile.web.ui.client.resources.ProfileMessages"/>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="errorPanel"/>
		<div class="deployment project-admin left">
			<h3><ui:text from="{pm.teamManagement}"/></h3>
			<g:FlowPanel ui:field="teamMembersPanel"/>
			<g:SimplePanel ui:field="addMemberPanel"/>
		</div>
		<!-- Project Tab Contents End -->
		<div class="project-help right">
			<p class="note"><ui:text from="{pm.inviteMemberNote}"/></p>
		</div>
		<div class="clear"></div>
	</g:HTMLPanel>
</ui:UiBinder>
