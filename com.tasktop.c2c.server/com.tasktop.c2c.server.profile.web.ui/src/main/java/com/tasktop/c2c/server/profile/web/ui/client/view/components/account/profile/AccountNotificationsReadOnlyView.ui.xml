<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'
			 xmlns:d='urn:import:com.google.gwt.dom.client'>
	<ui:with field="pm" type="com.tasktop.c2c.server.profile.web.ui.client.resources.ProfileMessages"/>
	<g:HTMLPanel>
				<div class="clear"></div><!-- Project Tab Navigation End-->
	
	<!-- Account Profile  Contents start-->
			<div class="deployment">
				<p><g:HTML ui:field="emailNotificationsSentToLabel"/></p>
			  <div class="notifications">
			    <h3>
			      <span class="heading left first">&nbsp;</span>
			      <span class="heading left"><ui:text from="{pm.email}"/></span>
			      <span class="heading right last">
			      	<g:Anchor ui:field="editButton" styleName="misc-icon edit"><span></span><ui:text from="{pm.edit}"/></g:Anchor>
			      </span>
			      <div class="clear"></div>
			    </h3>
			    <div class="row">
			      <span class="heading left"><ui:text from="{pm.notificationsTaskUpdatesAttachmentsComments}"/></span>
			      <span ui:field="emailTaskActivityCheckboxSpan" class="checkbox-item left active"><span></span></span>
			      <div class="clear"></div>
	          	</div>
	          	<div class="row">
			      <span class="heading left"><ui:text from="{pm.notificationsNewFeaturesTipsEvents}"/></span>
			      <span ui:field="emailNewsAndEventsCheckboxSpan" class="checkbox-item left active"><span></span></span>
			      <div class="clear"></div>
	          	</div>
	          	<div class="row">
			      <span class="heading left"><ui:text from="{pm.notificationsServiceMaintenanceUpdates}"/></span>
			      <span ui:field="emailServiceAndMaintenanceCheckboxSpan" class="checkbox-item left active"><span></span></span>
			      <div class="clear"></div>
	          	</div>
			  </div>
			</div><!-- Project Tab Contents start-->

		
	</g:HTMLPanel>
</ui:UiBinder>