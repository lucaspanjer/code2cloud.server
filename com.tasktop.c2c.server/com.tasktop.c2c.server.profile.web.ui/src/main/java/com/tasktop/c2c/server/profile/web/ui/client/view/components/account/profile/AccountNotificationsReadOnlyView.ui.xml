<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'
			 xmlns:d='urn:import:com.google.gwt.dom.client'>
	<g:HTMLPanel>
				<div class="clear"></div><!-- Project Tab Navigation End-->
	
	<!-- Account Profile  Contents start-->
			<div class="deployment">
				<p>
                   Email notifications will be sent to <span style="font-weight:bold"><g:Label ui:field="emailLabel"/></span>.
				</p>
			  <div class="notifications">
			    <h3>
			      <span class="heading left first">&nbsp;</span>
			      <span class="heading left">Email</span>
			      <span class="heading right last">
			      	<g:Anchor ui:field="editButton" styleName="misc-icon edit"><span></span>Edit</g:Anchor>
			      </span>
			      <div class="clear"></div>
			    </h3>
			    <div class="row">
			      <span class="heading left">Task updates, attachments and comments</span>
			      <span ui:field="emailTaskActivityCheckboxSpan" class="checkbox-item left active"><span></span></span>
			      <div class="clear"></div>
	          	</div>
	          	<div class="row">
			      <span class="heading left">New features, tips, and events</span>
			      <span ui:field="emailNewsAndEventsCheckboxSpan" class="checkbox-item left active"><span></span></span>
			      <div class="clear"></div>
	          	</div>
	          	<div class="row">
			      <span class="heading left">Service and system maintenance updates</span>
			      <span ui:field="emailServiceAndMaintenanceCheckboxSpan" class="checkbox-item left active"><span></span></span>
			      <div class="clear"></div>
	          	</div>
			  </div>
			</div><!-- Project Tab Contents start-->

		
	</g:HTMLPanel>
</ui:UiBinder>