<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
			 xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<div class="team-member">
			<span class="name">
				<g:Label ui:field="name"/>
				<em>(<g:Label ui:field="username"/>)
				</em>
			</span>
			<span class="email">
				<g:Label ui:field="email"/>
			</span>
			<span class="actions">
				<g:Label ui:field="role"/>
				<g:ListBox ui:field="roleListBox"/>
				<g:Anchor ui:field="removeButton" addStyleNames="remove">Remove</g:Anchor>
			</span>
			<div class="clear"></div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
