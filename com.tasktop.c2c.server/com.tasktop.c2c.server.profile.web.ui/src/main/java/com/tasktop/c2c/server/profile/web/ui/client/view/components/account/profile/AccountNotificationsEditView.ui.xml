<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field="pm" type="com.tasktop.c2c.server.profile.web.ui.client.resources.ProfileMessages"/>
	<g:HTMLPanel>
		<div class="clear"></div><!-- Project Tab Navigation End-->
			<!-- Account Profile  Contents start-->
			<div class="deployment">
				<p><g:HTML ui:field="emailNotificationsSentToLabel"/></p>
			  	<div class="notifications edit">
			    <h3>
			      <span class="heading left first">&nbsp;</span>
			      <span class="heading left"><ui:text from="{pm.email}"/></span>
			      <span class="heading right last"><ui:text from="{pm.editing}"/></span>
			      <div class="clear"></div>
			    </h3>
			    <div class="row">
			      <span class="heading left"><ui:text from="{pm.notificationsTaskUpdatesAttachmentsComments}"/></span>
			      <span class="checkbox-item left"><g:CheckBox ui:field="emailTaskActivityField"/></span>
			      <div class="clear"></div>
			    </div>
	          	<div class="row">
			      <span class="heading left"><ui:text from="{pm.notificationsNewFeaturesTipsEvents}"/></span>
			      <span class="checkbox-item left"><g:CheckBox ui:field="emailNewsAndEventsField"/></span>
			      <div class="clear"></div>
	          	</div>
	            <div class="row">
			      <span class="heading left"><ui:text from="{pm.notificationsServiceMaintenanceUpdates}"/></span>
			      <span class="checkbox-item left"><g:CheckBox ui:field="emailServiceAndMaintenanceField"/></span>
			      <div class="clear"></div>
	            </div>
          	    <div class="save">
				  <g:Button ui:field="cancelButton" styleName="right button marginleft" text="{pm.cancel}"/>
				  <g:Button ui:field="saveButton" styleName="right button special" text="{pm.save}"/>
				  <div class="clear"></div>
			    </div>
			  </div>
			</div><!-- Project Tab Contents start-->
		</g:HTMLPanel>
</ui:UiBinder>