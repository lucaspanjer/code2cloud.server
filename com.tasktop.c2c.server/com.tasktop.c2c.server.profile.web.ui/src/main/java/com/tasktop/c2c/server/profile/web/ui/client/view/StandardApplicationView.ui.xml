<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
   	<ui:with field='res' type='com.tasktop.c2c.server.common.profile.web.client.AppResources'/>
	   
		<g:HTMLPanel>
			<div class="{res.appCss.headerWrapper}">
				<g:SimplePanel ui:field="headerContainer"/>
				<g:SimplePanel ui:field="menuContainer"/>
			</div>
			<div class="{res.appCss.container}">
				<g:SimplePanel ui:field="contentContainer" />
			</div>

		    <div class="{res.appCss.footerStretch}">
		      <div class="{res.appCss.footer}">
		      	<g:SimplePanel ui:field="footerContainer"/>
		      </div>
		    </div>		

		</g:HTMLPanel>
</ui:UiBinder>