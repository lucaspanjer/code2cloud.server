<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field="pm" type="com.tasktop.c2c.server.profile.web.ui.client.resources.ProfileMessages"/>
	<g:HTMLPanel>
	
					<div class="project-filter">
						<ul class="right filter-activity ">
						  <li><ui:text from="{pm.show}"/>:</li>
						  	<li><g:CheckBox ui:field="buildToggleButton" text="{pm.build}" /></li>
							<li><g:CheckBox ui:field="wikiToggleButton" text="{pm.wiki}" /></li>
							<li><g:CheckBox ui:field="taskToggleButton" text="{pm.tasks}" /></li>
							<li><g:CheckBox ui:field="scmToggleButton" text="{pm.source}" /></li>
						</ul>
						<div class="clear"></div>
					</div>
        <div class="project-latest-activity">
			<h4><ui:text from="{pm.recentActivity}"/></h4>
		  	<g:FlowPanel ui:field="activityPanel" addStyleNames="dashboardActivityPanel"/>
	  	</div>
	</g:HTMLPanel>
</ui:UiBinder>