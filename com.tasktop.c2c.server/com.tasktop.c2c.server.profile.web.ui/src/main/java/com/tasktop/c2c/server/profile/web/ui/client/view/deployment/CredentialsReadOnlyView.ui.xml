<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
<ui:with field="pm" type="com.tasktop.c2c.server.profile.web.ui.client.resources.ProfileMessages"/>
<g:HTMLPanel>
	<fieldset>	
		<label><ui:text from="{pm.serverUrl}"/><ui:text from="{pm.colon}"/></label>
		<div><g:Label ui:field="serverUrl"/></div>
	</fieldset>
	<fieldset>
		<label><ui:text from="{pm.userName}"/><ui:text from="{pm.colon}"/></label>
		<div><g:Label ui:field="username"/></div>
	</fieldset>
</g:HTMLPanel>
</ui:UiBinder>	
