<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'
			 xmlns:cellview="urn:import:com.google.gwt.user.cellview.client">
	<ui:with field="tm" type="com.tasktop.c2c.server.tasks.client.TasksMessages"/>

	<g:HTMLPanel>
		<div class="clear"/>
		<div class="menu-items">
			<div class="top">
				<h5><ui:text from="{tm.tags}"/>
					<g:Anchor addStyleNames="right action add-deployment" ui:field="addKeyword">+</g:Anchor>
				</h5>
			</div>
			<cellview:CellList ui:field="keywordList"/>
			<div class="bottom"/>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>