<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
			 xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:with field="tm" type="com.tasktop.c2c.server.tasks.client.TasksMessages"/>
	<g:HTMLPanel>
	
		<ul class="inline-tabs">
			<li class="">
				<g:Anchor addStyleNames="button" ui:field="generalAdminAnchor"><ui:text from="{tm.general}"/></g:Anchor>
			</li>
			<li>
				<g:Anchor addStyleNames="button" ui:field="productsAdminAnchor"><ui:text from="{tm.products}"/></g:Anchor>
			</li>
			<li>
				<g:Anchor addStyleNames="button" ui:field="iterationsAdminAnchor"><ui:text from="{tm.iterations}"/></g:Anchor>
			</li>
			<li>
				<g:Anchor addStyleNames="button" ui:field="keywordsAdminAnchor"><ui:text from="{tm.tags}"/></g:Anchor>
			</li>
			<li>
				<g:Anchor addStyleNames="button" ui:field="customFieldsAdminAnchor"><ui:text from="{tm.customFields}"/></g:Anchor>
			</li>
		</ul>
	</g:HTMLPanel>
</ui:UiBinder>