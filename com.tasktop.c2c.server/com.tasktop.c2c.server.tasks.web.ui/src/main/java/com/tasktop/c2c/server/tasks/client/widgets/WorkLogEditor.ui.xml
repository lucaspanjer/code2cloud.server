<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:d="urn:import:com.google.gwt.user.datepicker.client"
	xmlns:t="urn:import:com.tasktop.c2c.server.common.web.client.widgets.time">
	<ui:with field="cpm" type="com.tasktop.c2c.server.common.profile.web.client.CommonProfileMessages"/>
	<ui:with field="tm" type="com.tasktop.c2c.server.tasks.client.TasksMessages"/>
	<g:HTMLPanel>
		<ui:text from="{cpm.date}"/> <d:DateBox ui:field="date"/>
		<ui:text from="{tm.hours}"/> <t:TimePeriodBox ui:field="hours"/>
		<ui:text from="{tm.description}"/>	<g:TextBox ui:field="comment"/>
	</g:HTMLPanel>
</ui:UiBinder>
